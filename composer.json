{
	"name": "suki/faq",
	"description": "SMF modification for creating Frequently Asked Questions page",
	"type": "smf-module",
	"license": "MPL-2.0",
	"authors": [
		{
			"name": "Michel Mendiola",
			"email": "suki@missallsunday.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Faq\\": "Sources/Faq/"
		}
	},
	"minimum-stability": "dev",
	"require": {
		"php": ">=8.0",
		"ext-ctype": "*"
	},
